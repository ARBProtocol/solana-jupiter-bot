		"axios": "1.1.3",
		"cross-env": "^7.0.3",
		"dotenv": "16.0.3",
		"file-stream-rotator": "^1.0.0",
		"millify": "^5.0.1",
		"socket.io-client": "^4.6.1",
		"ts-toolbelt": "^9.6.0"
	},
	"devDependencies": {
		"@types/node": "^18.11.10",
